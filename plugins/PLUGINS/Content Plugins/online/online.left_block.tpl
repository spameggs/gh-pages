<!-- who's online block --><table class="stats"><tr>	<td class="dark"><b>{$lang.online_statistics_text}</b></td></tr><tr>	<td class="dotted">		<span class="block_bg" style="padding: 0 5px 0 0;">{$lang.online_count_last_hour_text}</span>	</td>	<td class="column counter">{$onlineStatistics.lastHour}</td></tr><tr>	<td class="dotted">		<span class="block_bg" style="padding: 0 5px 0 0;">{$lang.online_count_last_day_text}</span>	</td>	<td class="column counter">{$onlineStatistics.lastDay}</td></tr></table><table class="stats"><tr>	<td class="dark"><b>{$lang.online_count_all_text|replace:'[number]':$onlineStatistics.total}</b></td></tr><tr>	<td class="dotted">		<span class="block_bg" style="padding: 0 5px 0 0;">{$lang.online_count_users_text}</span>	</td>	<td class="column counter">{$onlineStatistics.users}</td></tr><tr>	<td class="dotted">		<span class="block_bg" style="padding: 0 5px 0 0;">{$lang.online_count_guests_text}</span>	</td>	<td class="column counter">{$onlineStatistics.guests}</td></tr></table><!-- who's online block end -->